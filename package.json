{
  "name": "client_ts",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "NODE_OPTIONS='--experimental-specifier-resolution=node' ts-node src/server/index.ts",
    "build": "next build && tsc ",
    "start": "NODE_OPTIONS='--experimental-specifier-resolution=node' NODE_ENV=production node out/server/index.js",
    "lint": "next lint",
    "protoc": "grpc_tools_node_protoc --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=. src/proto/dockerGet/dockerGet.proto --ts_proto_opt=outputServices=grpc-js,esModuleInterop=true ",
    "e2e": "cypress run --spec cypress/integration/e2e/**/*.ts ",
    "e2e:after": "npm run  mochawesome-merge && npm run mochawesome-json-to-md && rm -rf cypress/reports/*.json",
    "mochawesome-merge": "mochawesome-merge cypress/reports/*.json -o cypress/reports/combined-report.json",
    "mochawesome-json-to-md": "mochawesome-json-to-md -p cypress/reports/combined-report.json -o cypress/reports/combined-report.md "
  },
